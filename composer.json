{
    "name": "illuminate/routing",
    "description": "A swift routing and dispatch component.",
    "keywords": ["laravel"],
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "require": {
        "illuminate/container": "1.2.*",
        "illuminate/http": "1.2.*",
        "illuminate/session": "1.2.*",
        "illuminate/support": "1.2.*",
        "symfony/http-foundation": "2.2.*",
        "symfony/http-kernel": "2.2.*",
        "symfony/routing": "2.2.*"
    },
    "require-dev": {
        "illuminate/console": "1.2.*",
        "illuminate/filesystem": "1.2.*",
        "mockery/mockery": "0.7.2"
    },
    "autoload": {
        "psr-0": {
            "Illuminate\\Routing": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "dev"
}
